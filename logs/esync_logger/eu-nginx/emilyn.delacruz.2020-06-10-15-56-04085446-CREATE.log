FILENAME: 80-443/eu888.vip.conf
-  
+  server {
+      listen 80;
+      server_name eu888.vip www.eu888.vip;
+      rewrite ^(.*)$ https://www.eu888.vip$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu888.vip www.eu888.vip;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu888.vip/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu888.vip/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


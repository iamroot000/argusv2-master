FILENAME: 80-443/18187000.com.conf
-  
+  server {
+      listen 80;
+      server_name 18187000.com www.18187000.com;
+      rewrite ^(.*)$ https://www.18187000.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 18187000.com www.18187000.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/18187000.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/18187000.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


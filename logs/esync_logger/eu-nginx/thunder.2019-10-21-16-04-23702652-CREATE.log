server {
    listen 80;
    server_name 888.v68.com;
    rewrite ^(.*)$ https://888.v68.com$1 permanent;
}

server {
    listen 443;
    server_name 888.v68.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/v68.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/v68.com/privkey.pem;
    include /usr/local/nginx/conf.d/include/eu_proxy_masterconfig.include;
}







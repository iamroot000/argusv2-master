FILENAME: 80-443/jbl00.com.conf
-  
+  server {
+      listen 80;
+      server_name jbl00.com www.jbl00.com;
+      rewrite ^(.*)$ https://www.jbl00.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jbl00.com www.jbl00.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jbl00.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jbl00.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo-test.include;
+  }
+  
+  


FILENAME: 80-443/eu79.co.conf
-  
+  server {
+      listen 80;
+      server_name eu79.co www.eu79.co;
+      rewrite ^(.*)$ https://www.eu79.co$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu79.co www.eu79.co;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu79.co/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu79.co/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


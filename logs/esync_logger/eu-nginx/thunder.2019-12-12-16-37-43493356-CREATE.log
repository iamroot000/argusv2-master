FILENAME: 80-443/v68pc.com.conf
-  
+  server {
+      listen 80;
+      server_name v68pc.com www.v68pc.com;
+      rewrite ^(.*)$ https://www.v68pc.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name v68pc.com www.v68pc.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/v68pc.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/v68pc.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/v68_xuanchuan_masterconfig.include;
+  }
+  
+  


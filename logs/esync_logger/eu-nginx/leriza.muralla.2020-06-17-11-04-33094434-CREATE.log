FILENAME: 80-443/eu10.net.conf
-  
+  server {
+      listen 80;
+      server_name eu10.net www.eu10.net;
+      rewrite ^(.*)$ https://www.eu10.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu10.net www.eu10.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu10.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu10.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


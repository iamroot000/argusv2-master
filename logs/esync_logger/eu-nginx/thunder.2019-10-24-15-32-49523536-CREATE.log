FILENAME: 80-443/v68app.com.conf
-  
+  server {
+      listen 80;
+      server_name v68app.com www.v68app.com;
+      rewrite ^(.*)$ https://www.v68app.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name v68app.com www.v68app.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/v68app.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/v68app.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/v68_xuanchuan_masterconfig.include;
+  }
+  
+  


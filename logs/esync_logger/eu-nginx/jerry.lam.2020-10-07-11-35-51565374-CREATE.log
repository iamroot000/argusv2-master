FILENAME: 80-443/eu6.co.conf
-  
+  server {
+      listen 80;
+      server_name eu6.co www.eu6.co;
+      rewrite ^(.*)$ https://www.eu6.co$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu6.co www.eu6.co;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu6.co/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu6.co/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu-xjw3-masterconfig.include;
+  }
+  
+  


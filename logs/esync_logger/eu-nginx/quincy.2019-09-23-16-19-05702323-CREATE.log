FILENAME: 80-443/eu2015.cn.conf
-  
+  server {
+      listen 80;
+      server_name eu2015.cn www.eu2015.cn;
+      rewrite ^(.*)$ https://www.eu2015.cn$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu2015.cn www.eu2015.cn;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu2015.cn/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu2015.cn/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xjw-master.include;
+  }
+  
+  


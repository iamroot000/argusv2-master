FILENAME: 80-443/eu8.me.conf
-  
+  server {
+      listen 80;
+      server_name eu8.me www.eu8.me;
+      rewrite ^(.*)$ https://www.eu8.me$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu8.me www.eu8.me;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu8.me/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu8.me/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


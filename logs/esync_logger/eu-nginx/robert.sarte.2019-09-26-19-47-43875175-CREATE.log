FILENAME: 80-443/eu02.cc.conf
-  
+  server {
+      listen 80;
+      server_name eu02.cc www.eu02.cc;
+      rewrite ^(.*)$ https://www.eu02.cc$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu02.cc www.eu02.cc;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu02.cc/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu02.cc/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


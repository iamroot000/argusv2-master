FILENAME: 80-443/eu38.us.conf
-  
+  server {
+      listen 80;
+      server_name eu38.us www.eu38.us;
+      rewrite ^(.*)$ https://www.eu38.us$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu38.us www.eu38.us;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu38.us/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu38.us/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


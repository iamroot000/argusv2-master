FILENAME: 80-443/eu178.com.conf
-  
+  server {
+      listen 80;
+      server_name eu178.com www.eu178.com;
+      rewrite ^(.*)$ https://www.eu178.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu178.com www.eu178.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu178.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu178.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


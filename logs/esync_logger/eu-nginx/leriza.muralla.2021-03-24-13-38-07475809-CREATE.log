FILENAME: 80-443/eu2018.net.conf
-  
+  server {
+      listen 80;
+      server_name eu2018.net www.eu2018.net;
+      rewrite ^(.*)$ https://www.eu2018.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu2018.net www.eu2018.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu2018.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu2018.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_official_masterconfig.include;
+  }
+  
+  


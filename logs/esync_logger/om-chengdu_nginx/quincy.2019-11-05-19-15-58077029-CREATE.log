--- /ansible/nginx/hostgroups/om-chengdu_nginx/nginx/conf.d/other/appcoffee.cn.conf	2019-10-30 14:24:10.689281570 +0800
+++ /tmp/appcoffee.cn.conf	2019-11-05 19:15:57.968179021 +0800
@@ -5,6 +5,15 @@
         error_log     logs/appcoffee.cn.error.log;
 
         location / {
+    
+            		add_header Access-Control-Allow-Origin *;
+        		add_header Access-Control-Allow-Methods 'GET, POST, OPTIONS';
+        		add_header Access-Control-Allow-Headers 'DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Authorization';
+
+        		if ($request_method = 'OPTIONS') {
+        		return 204;
+    			}
+
                 proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
                 proxy_set_header X-Real-IP  $remote_addr;
                 proxy_next_upstream http_502 http_504 http_404 error timeout invalid_header;



 server {
    listen       80;#HTTP Port
    server_name  .xbet.com;
    rewrite  ^(.*)$ https://www.xingtou.com$1      permanent; 
}

server {
    listen       443;#HTTP Port
    server_name  .xbet.com;
    rewrite  ^(.*)$ https://www.xingtou.com$1      permanent; 
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/xbet.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/xbet.com/privkey.pem;
}



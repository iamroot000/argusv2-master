FILENAME: 80-443/17play.org.conf
-  
+  server {
+      listen 80;
+      server_name 17play.org www.17play.org;
+      rewrite ^(.*)$ https://www.17play.org$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 17play.org www.17play.org;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/17play.org/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/17play.org/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xbet_official_masterconfig.include;
+  }
+  
+  


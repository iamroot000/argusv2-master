FILENAME: 80-443/xt876.com.conf
-  
+  server {
+      listen 80;
+      server_name xt876.com www.xt876.com;
+      rewrite ^(.*)$ https://www.xt876.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xt876.com www.xt876.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xt876.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xt876.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xbet_official_cdntest.include;
+  }
+  
+  


--- /ansible/nginx/hostgroups/wukongchat-prod/nginx/conf.d/other/wukong-logs.conf	2021-04-23 16:23:26.108797780 +0800
+++ /tmp/wukong-logs.conf	2021-04-23 16:28:34.558785283 +0800
@@ -1,4 +1,25 @@
 
+server {
+        listen       90;
+        server_name  logs.wukongchat.com;
+        index   index.jsp index.html index.htm;
+        charset utf-8;
+        log_not_found off;
+        location / {
+                root /usr/local/nginx/html;
+                index index.html index.htm;
+        }
 
 
+location  /wukongerrorlog-index {
+                alias /opt/container-logs/wukongchat;
+                default_type  text/plain;
+                fancyindex on;
+                fancyindex_exact_size off;
+                fancyindex_localtime on;
+                fancyindex_default_sort date_desc;
+        }
+
+}
+
 


--- /ansible/nginx/hostgroups/om-fpms_pms_nginx/nginx/conf.d/other/monitorsystem.conf	2020-08-19 14:06:05.171521483 +0800
+++ /tmp/monitorsystem.conf	2020-08-19 14:10:15.590976659 +0800
@@ -1,4 +1,34 @@
+server {
+    listen 443;
+    server_name monitorsystem-web.neweb.me;
+    ssl on;
+    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
+    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
+    
 
+	set $upstream monitorsystem-web;
+	set $upstream2 monitorsystem-server;
+	
+
+	location /server/ {
+  		rewrite ^/server/(.*) /$1 break;
+		proxy_pass http://$upstream2;
+  		 proxy_set_header Host  $host;
+   		 proxy_set_header X-Forwarded-For  $remote_addr;
+  		 proxy_set_header X-Forwarded-Proto $scheme ;
+	}
+
+	location / {
+  
+  			proxy_pass http://$upstream; 
+            proxy_set_header Host  $host;
+            proxy_set_header X-Forwarded-For  $remote_addr;
+            proxy_set_header X-Forwarded-Proto $scheme ;
+        }
+
+
+	
+}
 
 
 


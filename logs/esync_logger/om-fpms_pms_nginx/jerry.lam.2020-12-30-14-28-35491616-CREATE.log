--- /ansible/nginx/hostgroups/om-fpms_pms_nginx/nginx/conf.d/other/pay-web.conf	2020-12-30 14:25:35.718284370 +0800
+++ /tmp/pay-web.conf	2020-12-30 14:28:35.626196756 +0800
@@ -1,3 +1,30 @@
+server {
+    listen 443;
+    server_name yun885.cn;
+    ssl on;
+    ssl_certificate /usr/local/nginx/conf.d/ssl/yun885.cn/fullchain.pem;
+    ssl_certificate_key /usr/local/nginx/conf.d/ssl/yun885.cn/privkey.pem;
+    
+  
+  	access_log  logs/pay-web.access.log main;
+    access_log  logs/pay-web.logstash.access.log logstash;
+
+	
+
+	
+	
+	location / {
+  
+  			proxy_pass http://pay-web; 
+            proxy_set_header Host  $host;
+            proxy_set_header X-Forwarded-For  $remote_addr;
+            proxy_set_header X-Forwarded-Proto $scheme ;
+        }
+
+
+	
+}
+
 
 
 


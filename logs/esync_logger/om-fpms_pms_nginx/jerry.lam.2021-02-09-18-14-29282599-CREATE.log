server {
    listen 443;
    server_name 25187696.club;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/25187696.club/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/25187696.club/privkey.pem;
    
  
  	access_log  logs/25187696.access.log main;
    access_log  logs/25187696.logstash.access.log logstash;
  	error_log   logs/25187696.error.log ;

	set $upstream fun88;
	

	

	location / {
  
  			proxy_pass http://$upstream; 
            proxy_set_header Host  $host;
            proxy_set_header X-Forwarded-For  $remote_addr;
            proxy_set_header X-Forwarded-Proto $scheme ;
        }


	
}







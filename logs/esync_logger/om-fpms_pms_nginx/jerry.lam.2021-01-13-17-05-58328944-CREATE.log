server {
    listen 443;
    server_name manilatech1.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/manilatech1.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/manilatech1.com/privkey.pem;
    
  
  	access_log  logs/manilatech.access.log main;
    access_log  logs/manilatech.logstash.access.log logstash;

	set $upstream manilatech;
	

	

	location / {
  
  			proxy_pass http://$upstream; 
            proxy_set_header Host  $host;
            proxy_set_header X-Forwarded-For  $remote_addr;
            proxy_set_header X-Forwarded-Proto $scheme ;
        }


	
}








+server {
+        listen      443;
+        server_name api.paymeweb.co;
+        index   index.jsp index.html index.htm;
+        charset utf-8;
+        access_log  logs/api.paymeweb.co.access.log main;
+        ssl on;
+        ssl_certificate /usr/local/nginx/conf.d/ssl/paymeweb.co/fullchain.pem;
+        ssl_certificate_key /usr/local/nginx/conf.d/ssl/paymeweb.co/privkey.pem;
+  
+  		  location / {
+			proxy_redirect          off;
+			proxy_set_header        Host            $host;
+			proxy_set_header        X-Real-IP       $remote_addr;
+			proxy_set_header        referer         "";
+			proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;
+			proxy_connect_timeout   30;
+			proxy_send_timeout      30;
+			proxy_read_timeout      60;
+			client_max_body_size    30m;
+			client_body_buffer_size 256k;
+			proxy_buffer_size       256k;
+			proxy_buffers           4 256k;
+			proxy_busy_buffers_size 256k;
+			proxy_temp_file_write_size      256k;
+            proxy_pass http://pms2_9004;
+        }
+}
 

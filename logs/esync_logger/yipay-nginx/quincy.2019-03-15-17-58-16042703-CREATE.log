server {
    listen 80;
    server_name xn--ez-de3cx82g.com www.xn--ez-de3cx82g.com;
    rewrite ^(.*)$ https://www.xn--ez-de3cx82g.com$1 permanent;
}

server {
    listen       443;
    server_name www.xn--ez-de3cx82g.com xn--ez-de3cx82g.com;
	ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/xn--ez-de3cx82g.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/xn--ez-de3cx82g.com/privkey.pem;
    access_log  logs/pms-pay.access.log main;
    #return 307 https://$host$request_uri;
    location / {
        proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
        proxy_set_header X-Real-IP          $remote_addr;
        proxy_set_header Host               $host:$server_port;
        proxy_set_header Upgrade            $http_upgrade;
#        proxy_set_header Connection         $connection_upgrade;
        proxy_next_upstream http_502 http_504 http_404 error timeout invalid_header;
        proxy_http_version 1.1;
        proxy_connect_timeout   1800;
        proxy_send_timeout      1800;
        proxy_read_timeout      1800;
        proxy_pass http://pms-pay-web;
    }
  
	location /payment/ {
		proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
		#proxy_pass http://paynewwalter.neweb.me;
		proxy_pass http://cashiernewapi;
		
	}
  
    location /foundation/ {
		proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
		#proxy_pass http://paynewwalter.neweb.me;
		proxy_pass http://cashiernewapi;
	}
}





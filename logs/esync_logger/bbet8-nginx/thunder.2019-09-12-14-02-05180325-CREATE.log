FILENAME: 80-443/delivip9.com.conf
-  
+  server {
+      listen 80;
+      server_name delivip9.com www.delivip9.com;
+      rewrite ^(.*)$ https://www.delivip9.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name delivip9.com www.delivip9.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/delivip9.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/delivip9.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xdl_proxy_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/36516899.com.conf
-  
+  server {
+      listen 80;
+      server_name 36516899.com www.36516899.com;
+      rewrite ^(.*)$ https://www.36516899.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 36516899.com www.36516899.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/36516899.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/36516899.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/nav-xdl-pro.include;
+  }
+  
+  


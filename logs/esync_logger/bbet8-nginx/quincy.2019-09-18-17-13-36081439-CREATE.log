FILENAME: 80-443/deli2828.com.conf
-  
+  server {
+      listen 80;
+      server_name deli2828.com www.deli2828.com;
+      rewrite ^(.*)$ https://www.deli2828.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name deli2828.com www.deli2828.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/deli2828.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/deli2828.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/xindeli09.com.conf
-  
+  server {
+      listen 80;
+      server_name www.xindeli09.com;
+      rewrite ^(.*)$ https://www.xindeli09.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name www.xindeli09.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xindeli09.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xindeli09.com/privkey.pem;
+      include /usr/local/nginx/conf.d/include/xjw3-xdl-masterconfig.include;
+  }
+  

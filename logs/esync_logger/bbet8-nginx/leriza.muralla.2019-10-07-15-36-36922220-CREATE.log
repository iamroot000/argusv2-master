FILENAME: 80-443/xindeli08.com.conf
-  
+  server {
+      listen 80;
+      server_name xindeli08.com www.xindeli08.com;
+      rewrite ^(.*)$ https://www.xindeli08.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xindeli08.com www.xindeli08.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xindeli08.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xindeli08.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
+  }
+  
+  


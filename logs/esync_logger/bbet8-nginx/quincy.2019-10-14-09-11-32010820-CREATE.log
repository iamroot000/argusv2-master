server {
    listen 80;
    server_name test.neweb.com;
    rewrite ^(.*)$ https://test.neweb.me$1 permanent;
}

server {
    listen 443;
    server_name  test.neweb.me;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;

    if ($http_host !~* 'www') {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/weihu.include;
}





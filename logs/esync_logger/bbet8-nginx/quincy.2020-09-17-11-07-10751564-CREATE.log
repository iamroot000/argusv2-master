
server {
    listen 80;
    server_name zl.du87k.cn;
    rewrite ^(.*)$ https://zl.du87k.cn$1 permanent;
}

server {
    listen 443;
    server_name zl.du87k.cn;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/du87k.cn/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/du87k.cn/privkey.pem;


    include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
}





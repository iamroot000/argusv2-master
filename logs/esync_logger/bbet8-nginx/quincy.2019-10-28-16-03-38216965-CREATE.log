FILENAME: 80-443/opusgm.info.conf
-  
+  server {
+      listen 80;
+      server_name opusgm.info www.opusgm.info;
+      rewrite ^(.*)$ https://www.opusgm.info$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name opusgm.info www.opusgm.info;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/opusgm.info/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/opusgm.info/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xdl_proxy_official_masterconfig.include;
+  }
+  
+  


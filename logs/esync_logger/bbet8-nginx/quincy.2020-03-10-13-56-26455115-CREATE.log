
server {
    listen 80;
    server_name 36599990.com www.36599990.com;
    rewrite ^(.*)$ https://www.36516899.com$1 permanent;
}

server {
    listen 443;
    server_name 36599990.com www.36599990.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/36599990.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/36599990.com/privkey.pem;

    rewrite ^(.*)$ https://www.36516899.com$1 permanent;
}







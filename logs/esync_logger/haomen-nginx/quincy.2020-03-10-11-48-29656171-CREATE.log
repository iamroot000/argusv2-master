server {
    listen 80;
    server_name 12898888.com www.12898888.com;
    rewrite ^(.*)$ https://www.12859999.com$1 permanent;
}

server {
    listen 443;listen;
    server_name 12898888.com www.12898888.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/12898888.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/12898888.com/privkey.pem;

    rewrite ^(.*)$ https://www.12859999.com$1 permanent;
}





FILENAME: 80-443/haomenonline.com.conf
-  
+  server {
+      listen 80;
+      server_name haomenonline.com www.haomenonline.com;
+      rewrite ^(.*)$ https://www.haomenonline.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name haomenonline.com www.haomenonline.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/haomenonline.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/haomenonline.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_official_masterconfig.include;
+  }
+  
+  


server {
    listen 80;
    server_name 12858888.com www.12858888.com;
    rewrite ^(.*)$ https://www.12899888.com$1 permanent;
}

server {
    listen 443;
    server_name 12858888.com www.12858888.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/12858888.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/12858888.com/privkey.pem;

    rewrite ^(.*)$ https://www.12899888.com$1 permanent;
}






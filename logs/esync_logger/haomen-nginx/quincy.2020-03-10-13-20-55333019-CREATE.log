FILENAME: 80-443/12899888.com.conf
-  
+  server {
+      listen 80;
+      server_name 12899888.com www.12899888.com;
+      rewrite ^(.*)$ https://www.12899888.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 12899888.com www.12899888.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/12899888.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/12899888.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/nav-byl-pro.include;
+  }

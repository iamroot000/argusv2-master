FILENAME: 80-443/haomeninc.com.conf
-  
+  server {
+      listen 80;
+      server_name haomeninc.com www.haomeninc.com;
+      rewrite ^(.*)$ https://www.haomeninc.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name haomeninc.com www.haomeninc.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/haomeninc.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/haomeninc.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_official_masterconfig.include;
+  }
+  
+  


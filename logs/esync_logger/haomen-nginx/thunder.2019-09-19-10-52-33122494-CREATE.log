FILENAME: 80-443/byl.best.conf
-  
+  server {
+      listen 80;
+      server_name byl.best www.byl.best;
+      rewrite ^(.*)$ https://www.byl.best$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name byl.best www.byl.best;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/byl.best/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/byl.best/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/byl_proxy_official_masterconfig.include;
+  }
+  
+  


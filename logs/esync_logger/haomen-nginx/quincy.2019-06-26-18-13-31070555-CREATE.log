

server {
    listen 443;
    server_name test.neweb.com ;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.com/privkey.pem;

    if ($http_host !~* 'www') {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/haomen_weihu.include;
}





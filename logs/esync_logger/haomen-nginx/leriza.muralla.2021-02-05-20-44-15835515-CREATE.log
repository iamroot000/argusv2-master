server {
    listen 80;
    server_name b2288a.com www.b2288a.com;
    rewrite ^(.*)$ https://www.b8989a.com$1 permanent;
}

server {
    listen 443;listen 8988;
    server_name b2288a.com www.b2288a.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/b2288a.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/b2288a.com/privkey.pem;

    if ($http_host !~* 'www') {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/haomen_official_masterconfig.include;
}

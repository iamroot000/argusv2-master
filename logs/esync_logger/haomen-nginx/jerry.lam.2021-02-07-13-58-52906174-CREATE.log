    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/55222000.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/55222000.com/privkey.pem;

    rewrite ^(.*)$ https://55222.b1056.com$1 permanent;
}





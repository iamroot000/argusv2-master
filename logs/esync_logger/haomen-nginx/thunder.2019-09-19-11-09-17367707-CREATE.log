FILENAME: 80-443/baoyunlai-daili2.com.conf
-  
+  server {
+      listen 80;
+      server_name baoyunlai-daili2.com www.baoyunlai-daili2.com;
+      rewrite ^(.*)$ https://www.baoyunlai-daili2.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name baoyunlai-daili2.com www.baoyunlai-daili2.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/baoyunlai-daili2.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/baoyunlai-daili2.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/byl_proxy_official_masterconfig.include;
+  }
+  
+  


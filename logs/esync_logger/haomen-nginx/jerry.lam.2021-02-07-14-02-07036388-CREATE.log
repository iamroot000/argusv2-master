server {
    listen 80;
    server_name 55322000.com www.55322000.com;
    rewrite ^(.*)$ https://www.b0716.com$1 permanent;
}

server {
    listen 443;listen 8988;
    server_name 55322000.com www.55322000.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/55322000.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/55322000.com/privkey.pem;

    rewrite ^(.*)$ https://www.b0716.com$1 permanent;
}






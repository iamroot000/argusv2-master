FILENAME: 80-443/baoyunlai38.com.conf
-  
+  server {
+      listen 80;
+      server_name baoyunlai38.com www.baoyunlai38.com;
+      rewrite ^(.*)$ https://www.baoyunlai38.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name baoyunlai38.com www.baoyunlai38.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/baoyunlai38.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/baoyunlai38.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_official_masterconfig.include;
+  }
+  
+  


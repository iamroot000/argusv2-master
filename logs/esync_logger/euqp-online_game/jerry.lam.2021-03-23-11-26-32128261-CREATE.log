server {
    listen 80;
    server_name 10106.vip www.10106.vip;
    rewrite ^(.*)$ https://10106.eu602.com$1 permanent;
}

server {
    listen 443;
    server_name 10106.vip www.10106.vip;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/10106.vip/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/10106.vip/privkey.pem;

  
  	rewrite ^(.*)$ https://10106.eu602.com$1 permanent;
}





server {
    listen 80;
    server_name 10116.vip www.10116.vip;
    rewrite ^(.*)$ https://10116.eu602.com$1 permanent;
}

server {
    listen 443;
    server_name 10116.vip www.10116.vip;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/10116.vip/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/10116.vip/privkey.pem;

  	rewrite ^(.*)$ https://10116.eu602.com$1 permanent;
    #if ($http_host !~* 'www') {
      #  rewrite ^(.*)$ https://www.$http_host$1 permanent;
   # }

    #include /usr/local/nginx/conf.d/include/euqp_masterconf.include;
}







server {
    listen 80;
    server_name eu988.net;
    rewrite ^(.*)$ https://eu988.net$1 permanent;
}

server {
    listen 80;
    server_name www.eu988.net;
    rewrite ^(.*)$ https://www.eu988.net$1 permanent;
}


server {
    listen 443;listen 8808;
    server_name eu988.net www.eu988.net;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/eu988.net/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu988.net/privkey.pem;

    include /usr/local/nginx/conf.d/include/euqp_masterconf.include;
}







server {
    listen 80;
    server_name 10522.vip www.10522.vip;
    rewrite ^(.*)$ https://10522.eu602.com$1 permanent;
}

server {
    listen 443;
    server_name 10522.vip www.10522.vip;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/10522.vip/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/10522.vip/privkey.pem;

    rewrite ^(.*)$ https://10522.eu602.com$1 permanent;
}






server {
    listen 80;
    server_name 10305.vip www.10305.vip;
    rewrite ^(.*)$ https://10305.eu602.com$1 permanent;
}

server {
    listen 443;
    server_name 10305.vip www.10305.vip;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/10305.vip/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/10305.vip/privkey.pem;

   	rewrite ^(.*)$ https://10305.eu602.com$1 permanent;

}






FILENAME: 80-443/kblqqal.com.conf
-  
+  server {
+      listen 80;
+      server_name kblqqal.com www.kblqqal.com;
+      rewrite ^(.*)$ https://www.kblqqal.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name kblqqal.com www.kblqqal.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/kblqqal.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/kblqqal.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_masterconfig.include;
+  }
+  
+  


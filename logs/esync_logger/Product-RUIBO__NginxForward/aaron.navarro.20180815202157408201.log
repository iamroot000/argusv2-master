FILENAME: 80-443/jblsmsu.com.conf
-  
+  server {
+      listen 80;
+      server_name jblsmsu.com www.jblsmsu.com;
+      rewrite ^(.*)$ https://www.jblsmsu.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jblsmsu.com www.jblsmsu.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jblsmsu.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jblsmsu.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_brsong_masterconfig.include;
+  }
+  
+  


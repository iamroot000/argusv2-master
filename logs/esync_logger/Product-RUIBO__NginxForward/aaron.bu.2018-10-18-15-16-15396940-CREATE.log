FILENAME: 80-443/cmb222.com.conf
-  
+  server {
+      listen 80;
+      server_name cmb222.com www.cmb222.com;
+      rewrite ^(.*)$ https://www.cmb222.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name cmb222.com www.cmb222.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/cmb222.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/cmb222.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_ebetbw_masterconfig.include;
+  }
+  
+  


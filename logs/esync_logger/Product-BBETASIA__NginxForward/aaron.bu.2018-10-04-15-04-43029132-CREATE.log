FILENAME: 80-443/cmb666.com.conf
-  
+  server {
+      listen 80;
+      server_name cmb666.com www.cmb666.com;
+      rewrite ^(.*)$ https://www.cmb666.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name cmb666.com www.cmb666.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/cmb666.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/cmb666.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_jshbw_masterconfig.include;
+  }
+  
+  


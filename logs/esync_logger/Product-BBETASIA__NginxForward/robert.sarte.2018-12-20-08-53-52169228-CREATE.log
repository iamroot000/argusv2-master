FILENAME: 80-443/jin157.com.conf
-  
+  server {
+      listen 80;
+      server_name jin157.com www.jin157.com;
+      rewrite ^(.*)$ https://www.jin157.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jin157.com www.jin157.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jin157.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jin157.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


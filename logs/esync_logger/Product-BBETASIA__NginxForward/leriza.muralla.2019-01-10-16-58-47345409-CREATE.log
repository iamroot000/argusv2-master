FILENAME: 80-443/a1007.com.conf
-  
+  server {
+      listen 80;
+      server_name a1007.com www.a1007.com;
+      rewrite ^(.*)$ https://www.a1007.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name a1007.com www.a1007.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/a1007.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/a1007.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


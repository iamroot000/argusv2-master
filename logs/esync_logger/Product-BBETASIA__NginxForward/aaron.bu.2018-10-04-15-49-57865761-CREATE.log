FILENAME: 80-443/callmeboss168.com.conf
-  
+  server {
+      listen 80;
+      server_name callmeboss168.com www.callmeboss168.com;
+      rewrite ^(.*)$ https://www.callmeboss168.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name callmeboss168.com www.callmeboss168.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/callmeboss168.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/callmeboss168.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_jshbw_masterconfig.include;
+  }
+  
+  


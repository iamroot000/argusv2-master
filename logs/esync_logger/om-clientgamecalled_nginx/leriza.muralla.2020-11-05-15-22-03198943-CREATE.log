+server {
+        server_name ebet.lisboa.gapi.cpms8.me;
+  if ($time_iso8601 ~ "^(\d{4})-(\d{2})-(\d{2})T(\d{2})") {set $year $1;set $month $2;set $day $3;set $hour $4;}

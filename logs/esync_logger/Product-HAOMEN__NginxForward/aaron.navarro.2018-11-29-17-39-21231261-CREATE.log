FILENAME: 80-443/bao238.com.conf
-  
+  server {
+      listen 80;
+      server_name bao238.com www.bao238.com;
+      rewrite ^(.*)$ https://www.bao238.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name bao238.com www.bao238.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/bao238.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/bao238.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_official_masterconfig.include;
+  }
+  
+  


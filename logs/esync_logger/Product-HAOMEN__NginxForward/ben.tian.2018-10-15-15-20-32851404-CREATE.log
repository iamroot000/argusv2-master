FILENAME: 80-443/cmbdemo04.com.conf
-  
+  server {
+      listen 80;
+      server_name cmbdemo04.com www.cmbdemo04.com;
+      rewrite ^(.*)$ https://www.cmbdemo04.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name cmbdemo04.com www.cmbdemo04.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/cmbdemo04.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/cmbdemo04.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_baowangconfig.include;
+  }
+  
+  


FILENAME: 80-443/cmbdemo03.com.conf
-  
+  server {
+      listen 80;
+      server_name cmbdemo03.com www.cmbdemo03.com;
+      rewrite ^(.*)$ https://www.cmbdemo03.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name cmbdemo03.com www.cmbdemo03.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/cmbdemo03.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/cmbdemo03.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/haomen_baowangconfig.include;
+  }
+  
+  


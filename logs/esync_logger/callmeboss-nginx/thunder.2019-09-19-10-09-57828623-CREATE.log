FILENAME: 80-443/xpj.best.conf
-  
+  server {
+      listen 80;
+      server_name xpj.best www.xpj.best;
+      rewrite ^(.*)$ https://www.xpj.best$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xpj.best www.xpj.best;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xpj.best/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xpj.best/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xpj_proxy_official_masterconfig.include;
+  }
+  
+  


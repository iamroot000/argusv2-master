FILENAME: 80-443/npj0002.com.conf
-  
+  server {
+      listen 80;
+      server_name npj0002.com www.npj0002.com;
+      rewrite ^(.*)$ https://www.npj0002.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name npj0002.com www.npj0002.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/npj0002.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/npj0002.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xpj_xjw3.0_masterconfig.include;
+  }
+  
+  


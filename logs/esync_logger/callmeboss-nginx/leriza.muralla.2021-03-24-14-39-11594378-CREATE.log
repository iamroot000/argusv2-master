FILENAME: 80-443/cmb008.com.conf
-  
+  server {
+      listen 80;
+      server_name cmb008.com www.cmb008.com;
+      rewrite ^(.*)$ https://www.cmb008.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name cmb008.com www.cmb008.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/cmb008.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/cmb008.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/newpujing_official_masterconfig.include;
+  }
+  
+  


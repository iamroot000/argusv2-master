
server {
    listen 80;
    server_name www.wpc11.com;
    rewrite ^(.*)$ https://www.wpc11.com$1 permanent;
}

server {
    listen 443;
    server_name www.wpc11.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/wpc11.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/wpc11.com/privkey.pem;
    include /usr/local/nginx/conf.d/include/douji-web.include;
}





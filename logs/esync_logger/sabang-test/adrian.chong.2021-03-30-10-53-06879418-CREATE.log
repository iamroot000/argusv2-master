
server {
        listen         443 ssl;
        server_name    dj-cstest.payhome.site;
        index   index.jsp index.html index.htm;

        charset utf-8;
  
		ssl on;
    	ssl_certificate /usr/local/nginx/conf.d/ssl/payhome.site/fullchain.pem;
    	ssl_certificate_key /usr/local/nginx/conf.d/ssl/payhome.site/privkey.pem;
        access_log  logs/dj-pms2.access.log ;

         location ~ ^/(WEB-INF)/ {
            deny all;
         }

        location / {
            include /usr/local/nginx/conf.d/proxy.include;
    		proxy_set_header X-Forwarded-For $remote_addr;
    		proxy_pass http://127.0.0.1:9005;
        }

}





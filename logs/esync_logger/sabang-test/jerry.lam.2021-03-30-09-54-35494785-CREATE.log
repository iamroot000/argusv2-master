server {
        listen       90;
        server_name   cstest-dj.neweb.me;
        index   index.jsp index.html index.htm;
        charset utf-8;
        log_not_found off;
        location / {
                root /usr/local/nginx/html;
                index index.html index.htm;
        }

#FPMS LOGS
        location  /logs {
                alias /opt/logs/;
                default_type  text/plain;
                autoindex on;
                autoindex_exact_size off;
                #fancyindex on;
                #fancyindex_exact_size off;
                #fancyindex_localtime on;
                #fancyindex_default_sort date_desc;
        }
        location  /logs-index {
                alias /opt/logs/fpms_dj_logs/;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
#                fancyindex_default_sort date_desc;
        }

	location  /errorlog {
                alias /opt/container-logs/fpms;
                default_type  text/plain;
                autoindex on;
                autoindex_exact_size off;
        }
        location  /errorlog-index {
                alias /opt/container-logs/fpms;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }



#PMS LOGS
        location  /pms2logs-index {
                alias /opt/logs/pms2.0_dj_logs/;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }

        location  /errorlog-other {
                alias /opt/container-logs;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }

	location  /pms2errorlog-index {
                alias /opt/container-logs/pms2.0;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }

        location = /stub_status {
                stub_status;
        }

       # newdomain
       #location /domain {
       #      alias /usr/local/nginx/html ;
       #      index domain.html;
       #}

}





server {
    listen 80;
    server_name douji-p-proxy-test.neweb.me;
    rewrite ^(.*)$ https://douji-proxy-test.neweb.me$1 permanent;
}

server {
    listen 443;
    server_name douji-p-proxy-test.neweb.me;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
    include /usr/local/nginx/conf.d/include/douji-proxy-test.include;
}







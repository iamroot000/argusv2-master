FILENAME: 80-443/epk10a.com.conf
-  
+  server {
+      listen 80;
+      server_name epk10a.com www.epk10a.com;
+      rewrite ^(.*)$ https://www.epk10a.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name epk10a.com www.epk10a.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/epk10a.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/epk10a.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/xdl888.org.conf
-  
+  server {
+      listen 80;
+      server_name xdl888.org www.xdl888.org;
+      rewrite ^(.*)$ https://www.xdl888.org$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xdl888.org www.xdl888.org;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xdl888.org/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xdl888.org/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
+  }
+  
+  


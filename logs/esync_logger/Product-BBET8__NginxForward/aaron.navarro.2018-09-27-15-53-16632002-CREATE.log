FILENAME: 80-443/xdl888.net.conf
-  
+  server {
+      listen 80;
+      server_name xdl888.net www.xdl888.net;
+      rewrite ^(.*)$ https://www.xdl888.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xdl888.net www.xdl888.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xdl888.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xdl888.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbet8_official_masterconfig.include;
+  }
+  
+  


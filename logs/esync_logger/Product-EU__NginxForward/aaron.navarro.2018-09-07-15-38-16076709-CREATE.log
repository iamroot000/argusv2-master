FILENAME: 80-443/eu888999.com.conf
-  
+  server {
+      listen 80;
+      server_name eu888999.com www.eu888999.com;
+      rewrite ^(.*)$ https://www.eu888999.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu888999.com www.eu888999.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu888999.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu888999.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_lite_masterconfig.include;
+  }
+  
+  


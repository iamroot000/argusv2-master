FILENAME: 80-443/eu336.com.conf
-  
+  server {
+      listen 80;
+      server_name eu336.com www.eu336.com;
+      rewrite ^(.*)$ https://www.eu336.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name eu336.com www.eu336.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu336.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu336.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu_masterconfig.include;
+  }
+  
+  


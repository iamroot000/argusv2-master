server {
    listen 80;
    server_name 10879.vip www.10879.vip;
    rewrite ^(.*)$ https://10879.j72.me$1 permanent;
}

server {
    listen 443;listen 8818;
    server_name 10879.vip www.10879.vip;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/10879.vip/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/10879.vip/privkey.pem;

    rewrite ^(.*)$ https://10879.j72.me$1 permanent;
}






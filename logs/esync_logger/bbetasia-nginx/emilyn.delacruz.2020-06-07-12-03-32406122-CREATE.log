FILENAME: 80-443/jsh321.net.conf
-  
+  server {
+      listen 80;
+      server_name jsh321.net www.jsh321.net;
+      rewrite ^(.*)$ https://www.jsh321.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jsh321.net www.jsh321.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jsh321.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jsh321.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


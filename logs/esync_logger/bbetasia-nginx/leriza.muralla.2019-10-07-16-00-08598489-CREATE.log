FILENAME: 80-443/jinshihao11.com.conf
-  
+  server {
+      listen 80;
+      server_name jinshihao11.com www.jinshihao11.com;
+      rewrite ^(.*)$ https://www.jinshihao11.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jinshihao11.com www.jinshihao11.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jinshihao11.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jinshihao11.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


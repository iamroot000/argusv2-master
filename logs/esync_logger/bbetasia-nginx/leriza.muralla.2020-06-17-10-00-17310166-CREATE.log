FILENAME: 80-443/jin1.me.conf
-  
+  server {
+      listen 80;
+      server_name jin1.me www.jin1.me;
+      rewrite ^(.*)$ https://www.jin1.me$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jin1.me www.jin1.me;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jin1.me/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jin1.me/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/bzj13.com.conf
-  
+  server {
+      listen 80;
+      server_name bzj13.com www.bzj13.com;
+      rewrite ^(.*)$ https://www.bzj13.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name bzj13.com www.bzj13.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/bzj13.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/bzj13.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


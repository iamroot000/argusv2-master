FILENAME: 80-443/jsh.best.conf
-  
+  server {
+      listen 80;
+      server_name jsh.best www.jsh.best;
+      rewrite ^(.*)$ https://www.jsh.best$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jsh.best www.jsh.best;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jsh.best/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jsh.best/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_proxy_masterconfig.include;
+  }
+  
+  


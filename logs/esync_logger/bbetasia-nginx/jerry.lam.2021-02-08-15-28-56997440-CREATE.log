server {
    listen 80;
    server_name j72.me www.j72.me;
    rewrite ^(.*)$ https://www.j72.me$1 permanent;
}

server {
    listen 443;listen 8818;
    server_name j72.me www.j72.me;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/j72.me/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/j72.me/privkey.pem;

    if ($http_host !~* 'www') {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
}






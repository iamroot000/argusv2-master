FILENAME: 80-443/jinshihao16.com.conf
-  
+  server {
+      listen 80;
+      server_name jinshihao16.com www.jinshihao16.com;
+      rewrite ^(.*)$ https://www.jinshihao16.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jinshihao16.com www.jinshihao16.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jinshihao16.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jinshihao16.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/bbetasia_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/xbet4.com.conf
-  
+  server {
+      listen 80;
+      server_name xbet4.com www.xbet4.com;
+      rewrite ^(.*)$ https://www.xbet4.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name xbet4.com www.xbet4.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/xbet4.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/xbet4.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xbet_proxy_official_masterconfig.include;
+  }
+  
+  


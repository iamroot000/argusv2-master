server {
    listen 80;
    server_name xbet025.com www.xbet025.com;
    rewrite ^(.*)$ https://www.xbet025.com$1 permanent;
}

server {
    listen 443;listen 8188;
    server_name xbet025.com www.xbet025.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/xbet025.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/xbet025.com/privkey.pem;

    if ($http_host !~* 'www') {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/xbet_official_masterconfig.include;
}



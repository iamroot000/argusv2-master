server {
    listen 80;
    server_name .xbet29.com www.xbet29.com;
    #rewrite ^(.*)$ https://$http_host$1 redirect;
    return      301 https://$http_host$1;


}

server {
    listen 443;listen 8188;
    server_name .xbet29.com www.xbet29.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/xbet29.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/xbet29.com/privkey.pem;


    if ($http_host !~* 'www') { set $pp yes; }
    if ($http_host ~* 10990|10991|10995|59918|59879|55922|59878|59886|58030|55929|58033|58009|58010|58038|55966|58050|58055|58060|58065|58068|58070|58078|58095|58090|58089|58101|58128|58159|58138|59988000|59788000|59688000|59588000|55968000|55977000|55995000|55999000|55998000) {set $pp 'no'; }

    if ( $pp ~* 'yes' ) {
        rewrite ^(.*)$ https://www.$http_host$1 permanent;
    }

    include /usr/local/nginx/conf.d/include/xbet_official_masterconfig.include;
}






FILENAME: 80-443/gabo666.com.conf
-  
+  server {
+      listen 80;
+      server_name gabo666.com www.gabo666.com;
+      rewrite ^(.*)$ https://www.gabo666.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name gabo666.com www.gabo666.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/gabo666.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/gabo666.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/gabo_masterconfig.include;
+  }
+  
+  


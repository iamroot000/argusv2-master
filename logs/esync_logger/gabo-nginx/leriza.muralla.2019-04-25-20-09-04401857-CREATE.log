FILENAME: 80-443/jiabo28.net.conf
-  
+  server {
+      listen 80;
+      server_name jiabo28.net www.jiabo28.net;
+      rewrite ^(.*)$ https://www.jiabo28.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jiabo28.net www.jiabo28.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jiabo28.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jiabo28.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/jiabo_official_masterconfig.include;
+  }
+  
+  


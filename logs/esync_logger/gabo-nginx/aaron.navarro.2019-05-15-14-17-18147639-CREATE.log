FILENAME: 80-443/jiabo999.net.conf
-  
+  server {
+      listen 80;
+      server_name jiabo999.net www.jiabo999.net;
+      rewrite ^(.*)$ https://www.jiabo999.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jiabo999.net www.jiabo999.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jiabo999.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jiabo999.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/gabo_masterconfig.include;
+  }
+  
+  


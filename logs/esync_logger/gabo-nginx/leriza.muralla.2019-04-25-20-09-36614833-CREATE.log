FILENAME: 80-443/jiabbo98.net.conf
-  
+  server {
+      listen 80;
+      server_name jiabbo98.net www.jiabbo98.net;
+      rewrite ^(.*)$ https://www.jiabbo98.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jiabbo98.net www.jiabbo98.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jiabbo98.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jiabbo98.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/jiabo_official_masterconfig.include;
+  }
+  
+  


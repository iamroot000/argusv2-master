FILENAME: 80-443/jiabo12.com.conf
-  
+  server {
+      listen 80;
+      server_name jiabo12.com www.jiabo12.com;
+      rewrite ^(.*)$ https://www.jiabo12.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jiabo12.com www.jiabo12.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jiabo12.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jiabo12.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xjw3-jiabo-masterconfig.include;
+  }
+  
+  


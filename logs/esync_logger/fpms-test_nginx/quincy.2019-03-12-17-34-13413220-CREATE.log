--- /ansible/nginx/hostgroups/fpms-test_nginx/nginx/conf.d/other/payment-dev.neweb.me.conf	2019-03-12 17:32:02.873042198 +0800
+++ /tmp/payment-dev.neweb.me.conf	2019-03-12 17:34:13.344042198 +0800
@@ -71,3 +71,60 @@
     }
 }
 
+
+server {
+    listen       80;
+    server_name pms-pay-test.neweb.me;
+    access_log  logs/pms-pay-cstest.access.log main;
+    #return 307 https://$host$request_uri;
+    location / {
+        proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
+        proxy_set_header X-Real-IP          $remote_addr;
+        proxy_set_header Host               $host:$server_port;
+        proxy_set_header Upgrade            $http_upgrade;
+        proxy_set_header Connection         $connection_upgrade;
+        proxy_next_upstream http_502 http_504 http_404 error timeout invalid_header;
+        proxy_http_version 1.1;
+        proxy_connect_timeout   1800;
+        proxy_send_timeout      1800;
+        proxy_read_timeout      1800;
+        proxy_pass http://cstest-pms-pay-web;
+    }
+  
+	location /payment/ {
+		proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
+		#proxy_pass http://paynewwalter.neweb.me;
+		proxy_pass http://cstest-cashiernewapi;
+		
+	}
+  
+    location /foundation/ {
+		proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
+		#proxy_pass http://paynewwalter.neweb.me;
+		proxy_pass http://cstest-cashiernewapi;
+	}
+}
+server {
+    listen       443 ssl;
+    server_name pms-pay-test.neweb.me;
+    access_log  logs/pms-pay-cstest.access.log main;
+    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
+    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
+    location / {
+        proxy_set_header X-Forwarded-For    $proxy_add_x_forwarded_for;
+        proxy_set_header X-Real-IP          $remote_addr;
+        proxy_set_header Host               $host:$server_port;
+        proxy_set_header Upgrade            $http_upgrade;
+        proxy_set_header Connection         $connection_upgrade;
+        proxy_next_upstream http_502 http_504 http_404 error timeout invalid_header;
+        proxy_http_version 1.1;
+        proxy_connect_timeout   1800;
+        proxy_send_timeout      1800;
+        proxy_read_timeout      1800;
+        proxy_pass http://cstest-pms-pay-web;
+    }
+}

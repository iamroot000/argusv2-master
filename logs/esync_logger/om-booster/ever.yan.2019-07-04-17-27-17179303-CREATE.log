@@ -49,5 +49,85 @@
 
 }
 
+server {
+	listen       443;
+	server_name  vpn19.co www.vpn19.co;
 
+        #charset koi8-r;
+	ssl on;
+	ssl_certificate /usr/local/nginx/conf.d/ssl/vpn19.co/fullchain.pem;
+	ssl_certificate_key /usr/local/nginx/conf.d/ssl/vpn19.co/privkey.pem;
+
+	access_log  logs/access.log  main;
+	if ($http_host !~* 'www') {
+		rewrite ^(.*)$ https://www.$http_host$1 permanent;
+	}
+
+	location / {
+            #root   html/booster/;
+		root   /var/ftp/booster/mmexy/;
+		index  index.html index.htm;
+	}
+
+	error_page   500 502 503 504  /50x.html;
+	location = /50x.html {
+		root   html;
+	}
+
+}

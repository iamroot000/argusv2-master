--- /ansible/nginx/hostgroups/om-booster/nginx/conf.d/other/mmexy.com.conf	2019-04-16 10:22:53.376490795 +0800
+++ /tmp/mmexy.com.conf	2019-04-16 10:26:20.547490795 +0800
@@ -1,4 +1,36 @@
+server {
+    listen       80;
+    server_name mmexy.com www.mmexy.com;
+    rewrite  ^(.*)$  https://$http_host$1;
+}
 
 
+server {
+	listen       443;
+	server_name  mmexy.com www.mmexy.com;
+
+        #charset koi8-r;
+	ssl on;
+	ssl_certificate /usr/local/nginx/conf.d/ssl/mmexy.com/fullchain.pem;
+	ssl_certificate_key /usr/local/nginx/conf.d/ssl/mmexy.com/privkey.pem;
+
+	access_log  logs/access.log  main;
+	if ($http_host !~* 'www') {
+		rewrite ^(.*)$ https://www.$http_host$1 permanent;
+	}
+
+	location / {
+            #root   html/booster/;
+		root   /var/ftp/booster/mmexy;
+		index  index.html index.htm;
+	}
+
+	error_page   500 502 503 504  /50x.html;
+	location = /50x.html {
+		root   html;
+	}
+
+}
+
 
 


--- /ansible/nginx/hostgroups/om-live800_nginx/nginx/conf.d/include/domains.include	2019-08-30 11:11:43.812082747 +0800
+++ /tmp/domains.include	2019-08-30 11:12:10.731082747 +0800
@@ -1,3 +1,61 @@
+        charset utf-8;
+        access_log logs/80_access.log main;
+
+		 # cache.appcache, your document html and data
+        location ~* \.(?:manifest|appcache|html?|xml|json)$ {
+          expires -1;
+                proxy_pass http://chat800;
+        }
+
+         # Feed
+        location ~* \.(?:rss|atom)$ {
+                proxy_cache cache_one;
+                expires 1h;
+                add_header X-Proxy-Cache $upstream_cache_status;
+                add_header Cache-Control "public";
+                proxy_pass http://chat800;
+        }
+
+	 # Media: images, icons, video, audio, HTC
+        location ~* \.(?:jpg|jpeg|gif|png|ico|cur|gz|svg|svgz|mp4|ogg|ogv|webm|htc)$ {
+                proxy_cache cache_one;
+                proxy_cache_valid 200 304 12h; # for nginx proxy cache
+                proxy_cache_valid 301 302 12h;
+                proxy_cache_valid any 5m;
+                proxy_cache_valid 404      1m;
+                #proxy_cache_key $host$uri$is_args$args;
+                expires 1M;
+                add_header Cache-Control "public";
+                add_header X-Proxy-Cache $upstream_cache_status;
+                proxy_pass http://chat800;
+        }
+
+         # CSS and Javascript
+        location ~* \.(?:css|js)$ {
+                proxy_cache cache_one;
+                proxy_cache_valid 200 304 12h; # for nginx proxy cache
+                proxy_cache_valid 301 302 12h;
+                proxy_cache_valid any 5m;
+                proxy_cache_valid 404      1m;
+                #proxy_cache_key $host$uri$is_args$args;
+
+                expires 1y;
+                add_header Cache-Control "public";
+                add_header X-Proxy-Cache $upstream_cache_status;
+                proxy_pass http://chat800;
+                }
+	
+
+
+        location / {
+                include proxy.conf;
+        #        proxy_pass http://chat800;
+                proxy_pass http://chat800;
+
+                }
+
+        error_page 404 404.jsp;
+        error_page 500 502 503 500.jsp;
 
 
 


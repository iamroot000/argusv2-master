server {
    listen 80;
    server_name sabong81.com www.sabong81.com;
    rewrite ^(.*)$ https://www.sabong81.com$1 permanent;
}

server {
    listen 443;
    server_name www.sabong81.com;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/sabong81.com/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/sabong81.com/privkey.pem;
    include /usr/local/nginx/conf.d/include/douji-p-test.include;
}








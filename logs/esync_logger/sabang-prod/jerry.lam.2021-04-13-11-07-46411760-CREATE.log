server {
        listen       90;
        server_name   douji-logs.neweb.me;
        index   index.jsp index.html index.htm;
        charset utf-8;
        log_not_found off;
        location / {
                root html;
                index index.html index.htm;
        }

        location  /logs {
                alias /opt/logs/;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }
        location  /pms-logs {
                alias /opt/tomcat-logs/pms;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
       }

        location  /sms-logs {
                alias /opt/tomcat-logs/sms;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
       }
       location  /errorlog {
                alias /opt/container-logs;
                default_type  text/plain;
                fancyindex on;
                fancyindex_exact_size off;
                fancyindex_localtime on;
                fancyindex_default_sort date_desc;
        }
}









server {
        listen       80;
        server_name  ssl-ftp.neweb.me;
		gzip on;
        charset utf-8;

        location / {
        root /web/ssl/ssladmin;
	    autoindex on;
		autoindex_localtime on;
        }

        access_log  logs/ssladmin.access.log main;
        access_log  logs/ssladmin.logstash.access.log logstash;
        error_log  logs/ssladmin.error.log ;

    }





--- /ansible/nginx/hostgroups/acc-autopay_ftp/nginx/conf.d/other/autokey.conf	2019-04-03 15:34:11.349523282 +0800
+++ /tmp/autokey.conf	2019-04-03 15:34:56.210599163 +0800
@@ -1,4 +1,20 @@
+server {
+        listen       443;
+        server_name  autokey.neweb.me;
+        gzip on;
+        charset utf-8;
+		ssl on;
+		ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
+		ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
+        location / {
+			root /var/ftp/autopaykey;
+			#autoindex on;
+			#autoindex_localtime on;
+        }
+        access_log  logs/autokey.access.log main;
+        error_log  logs/autokey.error.log ;
 
+    }
 
 
 


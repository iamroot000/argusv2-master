server {
        listen       80;
        server_name  bankqr.pms8.me;
		gzip on;
        charset utf-8;
		rewrite ^(.*)$ https://bankqr.pms8.me$1 permanent;
}

server {
        listen       443;
        server_name     bankqr.pms8.me;
                gzip on;
        charset utf-8;
                ssl on;
                ssl_certificate /usr/local/nginx/conf.d/ssl/pms8.me/fullchain.pem;
                ssl_certificate_key /usr/local/nginx/conf.d/ssl/pms8.me/privkey.pem;
        location / {
        root /web/images/bankqr;
            autoindex on;
                autoindex_localtime on;
        }

        access_log  logs/bankqr.access.log main;
        error_log  logs/bankqr.error.log ;

    }





server {
        listen       80;
        server_name autopay-ftp.neweb.me;
		gzip on;
        charset utf-8;

        location / {
        root /web/apk/autopay;
	    autoindex on;
		autoindex_localtime on;
        }

        access_log  logs/autopay-ftp.access.log main;
        error_log  logs/autopay-ftp.error.log ;

    }

server {
        listen       80 ;
        server_name  autopay-front-test.neweb.me;
		gzip on;
        charset utf-8;
#  		ssl on;
#		ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
#		ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
    
    location  ^~ /js/chunk-8ec5f124.dbd9b8bd.js { 
            deny    all;
	}
  
        location / {
    
   
    	root /web/ssl/autopay-front-test;
#	    autoindex on;
#		autoindex_localtime on;
    
    
    index  index.html index.htm;
    if (!-e $request_filename) {
        rewrite ^/(.*) /index.html last;
        break;
    }
        }

        access_log  logs/autopay-test.access.log main;
        access_log  logs/autopay-test.logstash.access.log logstash;
        error_log  logs/autopay-test.error.log ;

    }






server {
        listen       80;
        server_name  ca.autopay8.me;
		gzip on;
        charset utf-8;

        location / {
        root /web/ssl/autopay-ca;
	    autoindex on;
		autoindex_localtime on;
        }

        access_log  logs/autopay-ca.access.log main;
        error_log  logs/autopay-ca.error.log ;

    }

+    	allow 101.78.133.210;
+    	allow 58.71.201.65;
+    	allow 211.24.72.105;
+    	allow 61.238.107.16;
+    	allow 52.184.38.87;
+   
+		deny all;
+   
+    	root /web/ssl/ssl-test-dj;

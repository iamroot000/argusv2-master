server {
    listen 80;
    server_name autopay-test.neweb.me;
    rewrite ^(.*)$ https://autopay-test.neweb.me$1 permanent;
}

server {
        listen       443 ssl;
        server_name  autopay-test.neweb.me;
		gzip on;
        charset utf-8;
  		ssl on;
		ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
		ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;

        location / {
    
   
    	root /web/ssl/autopay-test;
	    autoindex on;
		autoindex_localtime on;
        }

        access_log  logs/ssladmin.access.log main;
        access_log  logs/ssladmin.logstash.access.log logstash;
        error_log  logs/ssladmin.error.log ;

    }






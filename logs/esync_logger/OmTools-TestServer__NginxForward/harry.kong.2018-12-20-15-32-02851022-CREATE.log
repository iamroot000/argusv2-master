FILENAME: 443/eu6.me.conf
-  
+  server {
+      listen 443;
+      server_name eu6.me www.eu6.me;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/eu6.me/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/eu6.me/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/eu6.me.include;
+  }
+  
+  


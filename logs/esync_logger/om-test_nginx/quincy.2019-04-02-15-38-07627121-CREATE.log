server {
        listen         80;
        server_name    yifu-cstest.paymehome.co yifu2-cstest.paymehome.co yifu3-cstest.paymehome.co;
        index   index.jsp index.html index.htm;

        charset utf-8;

        access_log  logs/yifu-cstest.access.log ;

         location ~ ^/(WEB-INF)/ {
            deny all;
         }

        location / {
            include /usr/local/nginx/conf.d/proxy.include;
            proxy_pass http://test-yifu1-domain;
        }

}

server {
    listen 80;
    server_name xjw2-xdl-test.neweb.me;
    rewrite ^(.*)$ https://xjw2-xdl-test.neweb.me$1 permanent;
}

server {
    listen 443;
    server_name xjw2-xdl-test.neweb.me;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
    include /usr/local/nginx/conf.d/include/xjw2_xdl_test.include;
}





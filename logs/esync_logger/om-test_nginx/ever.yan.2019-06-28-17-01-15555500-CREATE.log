FILENAME: 80-443/neweb.me.conf
-  
+  server {
+      listen 80;
+      server_name qrserver.neweb.me;
+      rewrite ^(.*)$ https://qrserver.neweb.me$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name qrserver.neweb.me;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
+      include /usr/local/nginx/conf.d/include/qrsever.include;
+  }
+  

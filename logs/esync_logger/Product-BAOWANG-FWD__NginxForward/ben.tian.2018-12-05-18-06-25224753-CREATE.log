FILENAME: 80-443/by888106.com.conf
-  
+  server {
+      listen 80;
+      server_name by888106.com www.by888106.com;
+      rewrite ^(.*)$ https://www.by888106.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name by888106.com www.by888106.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/by888106.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/by888106.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/boyue_official_masterconfig.include;
+  }
+  

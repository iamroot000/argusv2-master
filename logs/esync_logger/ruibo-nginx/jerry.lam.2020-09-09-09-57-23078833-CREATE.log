server {
    listen 8088;
    server_name 80api.realmanplay.net;
    ssl on;
    ssl_certificate /usr/local/nginx/conf.d/ssl/realmanplay.net/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/realmanplay.net/privkey.pem;


    include /usr/local/nginx/conf.d/include/ruibo_official_masterconfig.include;
}





FILENAME: 80-443/b3003.com.conf
-  
+  server {
+      listen 80;
+      server_name b3003.com www.b3003.com;
+      rewrite ^(.*)$ https://www.b3003.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name b3003.com www.b3003.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/b3003.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/b3003.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_official_masterconfig.include;
+  }
+  
+  


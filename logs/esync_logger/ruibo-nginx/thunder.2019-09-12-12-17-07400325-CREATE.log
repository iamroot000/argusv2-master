FILENAME: 80-443/jbl.best.conf
-  
+  server {
+      listen 80;
+      server_name jbl.best www.jbl.best;
+      rewrite ^(.*)$ https://www.jbl.best$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name jbl.best www.jbl.best;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/jbl.best/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/jbl.best/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/jbl_proxy_official_masterconfig.include;
+  }
+  
+  


FILENAME: 80-443/king888888.net.conf
-  
+  server {
+      listen 80;
+      server_name king888888.net www.king888888.net;
+      rewrite ^(.*)$ https://www.king888888.net$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name king888888.net www.king888888.net;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/king888888.net/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/king888888.net/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_official_masterconfig.include;
+  }
+  
+  


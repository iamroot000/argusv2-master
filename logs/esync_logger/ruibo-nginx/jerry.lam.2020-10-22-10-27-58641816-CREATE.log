FILENAME: 80-443/18183888.com.conf
-  
+  server {
+      listen 80;
+      server_name 18183888.com www.18183888.com;
+      rewrite ^(.*)$ https://www.18183888.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 18183888.com www.18183888.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/18183888.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/18183888.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/nav-jbl-pro.include;
+  }

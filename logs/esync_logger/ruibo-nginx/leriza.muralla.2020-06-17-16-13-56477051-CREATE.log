FILENAME: 80-443/688995.com.conf
-  
+  server {
+      listen 80;
+      server_name 688995.com www.688995.com;
+      rewrite ^(.*)$ https://www.688995.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 688995.com www.688995.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/688995.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/688995.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/ruibo_official_masterconfig.include;
+  }
+  
+  


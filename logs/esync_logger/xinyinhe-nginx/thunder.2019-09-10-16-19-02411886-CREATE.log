FILENAME: 80-443/y91899.com.conf
-  
+  server {
+      listen 80;
+      server_name y91899.com www.y91899.com;
+      rewrite ^(.*)$ https://www.y91899.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name y91899.com www.y91899.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/y91899.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/y91899.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/xyh_official_masterconfig.include;
+  }
+  
+  


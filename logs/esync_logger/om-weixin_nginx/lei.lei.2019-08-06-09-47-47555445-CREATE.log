FILENAME: 80-443/573595.com.conf
-  
+  server {
+      listen 80;
+      server_name 573595.com www.573595.com;
+      rewrite ^(.*)$ https://www.573595.com$1 permanent;
+  }
+  
+  server {
+      listen 443;
+      server_name 573595.com www.573595.com;
+      ssl on;
+      ssl_certificate /usr/local/nginx/conf.d/ssl/573595.com/fullchain.pem;
+      ssl_certificate_key /usr/local/nginx/conf.d/ssl/573595.com/privkey.pem;
+  
+      if ($http_host !~* 'www') {
+          rewrite ^(.*)$ https://www.$http_host$1 permanent;
+      }
+  
+      include /usr/local/nginx/conf.d/include/573595.com.include;
+  }
+  
+  


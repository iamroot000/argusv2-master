server {
        listen     81;
        server_name gi.779boyue.com;
        index index.jsp index.html index.htm;
        charset utf-8;

        access_log  logs/gi.779boyue.com.access.log main;
        error_log logs/gi.779boyue.com.error.log;

        if ($request_method !~* GET|POST|HEAD) {
                return 403;
        }

        location / {
                proxy_redirect off;
                proxy_set_header        Host            gi.779boyue.com;
                proxy_set_header        X-Real-IP       $remote_addr;
                proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;
                client_max_body_size    10m;
                client_body_buffer_size 128k;
                proxy_connect_timeout   90;
                proxy_send_timeout      90;
                proxy_read_timeout      90;
                proxy_buffers           32 4k;
                proxy_pass http://gi.779boyue.com:81;
        }
        error_page   404 404.jsp;
        error_page   500 502 503 500.jsp;
} # server end



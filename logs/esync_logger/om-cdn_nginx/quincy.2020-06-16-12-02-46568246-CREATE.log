+server {
+    listen 443; 
+    server_name ssocdn2.99gov.cn;
+    charset utf-8;
+    access_log  logs/196311.com.access.log main;
+    error_log   logs/196311.com.error.log;
+    ssl on; 
+    ssl_certificate     /usr/local/nginx/conf.d/ssl/99gov.cn/fullchain.pem;
+    ssl_certificate_key /usr/local/nginx/conf.d/ssl/99gov.cn/privkey.pem;
+    location / {
+        proxy_pass https://www.inxedu.com/;
+    }
+    location ^~ /images/ {
+        alias /var/ftp/images/;
+    }
+  
+    location ^~ /cdn/ {
+        alias /var/ftp/allcdn/;
+	}
+  	location ^~ /nav-html/ {
+        alias /var/ftp/allcdn/nav-html/;
+	}
+}

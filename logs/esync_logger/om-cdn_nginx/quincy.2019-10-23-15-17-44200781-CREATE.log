server {
    listen 443; 
    server_name v68app.sanjiangjob.cn;
    charset utf-8;
    access_log  logs/yantaituotang.com.access.log main;
    error_log   logs/yantaituotang.com.error.log;
    ssl on; 
    ssl_certificate     /usr/local/nginx/conf.d/ssl/sanjiangjob.cn/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/sanjiangjob.cn/privkey.pem;
    location / {
        root /var/ftp/images/v68app;
    }





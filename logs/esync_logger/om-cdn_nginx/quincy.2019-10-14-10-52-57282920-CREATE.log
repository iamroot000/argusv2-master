server {
    listen 443; 
    server_name front-end-test.neweb.me;
    charset utf-8;
    access_log  logs/yantaituotang.com.access.log main;
    error_log   logs/yantaituotang.com.error.log;
    ssl on; 
    ssl_certificate     /usr/local/nginx/conf.d/ssl/neweb.me/fullchain.pem;
    ssl_certificate_key /usr/local/nginx/conf.d/ssl/neweb.me/privkey.pem;
    location / {
        alias /var/ftp/images/dev-test;
    }
#    location ^~ /images/ {
#        alias /var/ftp/images/;
#    }
  
#    location ^~ /cdn/ {
#        alias /var/ftp/allcdn/;
#	}
}





